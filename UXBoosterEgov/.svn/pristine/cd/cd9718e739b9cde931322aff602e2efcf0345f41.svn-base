/*********************************************************
* 5 사용자 FUNCTION 영역
********************************************************/

/* 폼목 그리드 숨김 처리  */
this.fnSetGrid = function()
{
	var colCnt = this.gridGds.getCellCount("head");
	var instCnt = this.dsInstCd.rowcount;
	var colSize = 35;
	
	// column size 설정
	if(instCnt < 6) colSize = colSize + ((6 - instCnt) * 10);
	
	for(var i = 0; i < this.dsInstCd.rowcount; i++)
	{
		for(var j = 0; j < colCnt; j++)
		{
			var strBindCol = this.gridGds.getCellProperty("body", j, "text");
			
			if(strBindCol != null)
			{
				strBindCol = strBindCol.substr(5);
				
				var strPosblQy = strBindCol.split("_POSBL_QY");
				var strPlanQy = strBindCol.split("_PLAN_QY");
				
				if(strPosblQy[0] == this.dsInstCd.getColumn(i, "CODE")) this.gridGds.setFormatColProperty(j, "size", colSize);
				if(strPlanQy[0] == this.dsInstCd.getColumn(i, "CODE")) this.gridGds.setFormatColProperty(j, "size", colSize);
				
				if(this.dsInstCd.getColumn(i, "CODE") == "INST_1" && this.gridGds.getCellProperty( "head", j, "text") == "INST_1") this.gridGds.setCellProperty( "head", j, "text", this.dsInstCd.getColumn(i, "NAME"));
				if(this.dsInstCd.getColumn(i, "CODE") == "INST_2" && this.gridGds.getCellProperty( "head", j, "text") == "INST_2") this.gridGds.setCellProperty( "head", j, "text", this.dsInstCd.getColumn(i, "NAME"));
				if(this.dsInstCd.getColumn(i, "CODE") == "INST_3" && this.gridGds.getCellProperty( "head", j, "text") == "INST_3") this.gridGds.setCellProperty( "head", j, "text", this.dsInstCd.getColumn(i, "NAME"));
				if(this.dsInstCd.getColumn(i, "CODE") == "INST_4" && this.gridGds.getCellProperty( "head", j, "text") == "INST_4") this.gridGds.setCellProperty( "head", j, "text", this.dsInstCd.getColumn(i, "NAME"));
				if(this.dsInstCd.getColumn(i, "CODE") == "INST_5" && this.gridGds.getCellProperty( "head", j, "text") == "INST_5") this.gridGds.setCellProperty( "head", j, "text", this.dsInstCd.getColumn(i, "NAME"));
				
			}
		}
	}
}

