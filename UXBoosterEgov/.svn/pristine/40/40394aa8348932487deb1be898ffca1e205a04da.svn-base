this.fnCallBack = function(sId, nErrCd, sErrMsg)
{
    var arrInfo = sId.split("_");
    var sSvcId = arrInfo[0];
    var objDs = this.objects[arrInfo[1]];
    
    trace(sSvcId+ " --> errorcd : (" + nErrCd + ")" + sErrMsg +" / rowcount : " + objDs.rowcount);
    
}
this.takeMultiCode_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.Dataset00.clearData();
    this.Dataset01.clearData();
    this.Dataset02.clearData();
    
    take.tranCode
        (
            this, 
            ["252","240","209"], 
            ["Dataset00","Dataset01","Dataset02"], 
            ["NO","ALL","NO"],
            function(sId, nErrCd, sErrMsg)
            {
                trace("Dataset00 --> " + this.Dataset00.saveXML());
                trace("Dataset01 --> " + this.Dataset01.saveXML());
                trace("Dataset02 --> " + this.Dataset02.saveXML());
            }
        );
};]]></Script>
