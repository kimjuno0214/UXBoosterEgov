this.Button00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
    this.Dataset00.clearData();
    this.Dataset01.clearData();
    this.Dataset02.clearData();
    this.Dataset03.clearData();
    this.Dataset04.clearData();
    this.Dataset05.clearData();
    this.Dataset06.clearData();
    this.Dataset07.clearData();
    this.Dataset08.clearData();
    this.Dataset09.clearData();
    this.Dataset10.clearData();
    
	take.tranCode(this, "252", "Dataset00", "", function (sId, nErrCd, sErrMsg) { trace("252 --> " + this.Dataset00.rowcount); });
    take.tranCode(this, "240", "Dataset01", "", function (sId, nErrCd, sErrMsg) { trace("240 --> " + this.Dataset01.rowcount); });
    take.tranCode(this, "209", "Dataset02", "", function (sId, nErrCd, sErrMsg) { trace("209 --> " + this.Dataset02.rowcount); });
    take.tranCode(this, "253", "Dataset03", "", function (sId, nErrCd, sErrMsg) { trace("253 --> " + this.Dataset03.rowcount); });
    take.tranCode(this, "072", "Dataset04", "", function (sId, nErrCd, sErrMsg) { trace("072 --> " + this.Dataset04.rowcount); });
    take.tranCode(this, "262", "Dataset05", "", function (sId, nErrCd, sErrMsg) { trace("262 --> " + this.Dataset05.rowcount); });
    take.tranCode(this, "263", "Dataset06", "", function (sId, nErrCd, sErrMsg) { trace("263 --> " + this.Dataset06.rowcount); });
    take.tranCode(this, "244", "Dataset07", "", function (sId, nErrCd, sErrMsg) { trace("244 --> " + this.Dataset07.rowcount); });
    take.tranCode(this, "245", "Dataset08", "", function (sId, nErrCd, sErrMsg) { trace("245 --> " + this.Dataset08.rowcount); });
    take.tranCode(this, "259", "Dataset09", "", function (sId, nErrCd, sErrMsg) { trace("259 --> " + this.Dataset09.rowcount); });
    take.tranSelect
        (
            this,  //Form 객체
            "test",
            "TakeSystemMapper.upMenuIdSelect",
            "",
            "Dataset10",
            "",
            function (sId, nErr, sMsg) //sCallBack
            {
                if (sId=="")
                {
                    trace("TakeSystemMapper.upMenuIdSelect --> " + this.Dataset10.rowcount);
                }
            }
        );
};

