						this.dsNotice.setColumn(0, 'BOARD_ATTACH', rtnValue.resultData);
						this.fv_divFile.fn_setAtchFileId(rtnValue.resultData);
						this.fv_divFile.fnSearchFileList(this);
					}
				);
