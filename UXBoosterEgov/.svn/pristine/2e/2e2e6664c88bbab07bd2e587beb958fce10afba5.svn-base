    //Transaction은 성공이나 실제 처리된게 없을 경우 처리
    if( sErrMsg == "SUCC" )
    {
        //Success Script
    } else {
        //Etc Script
    }

    switch(sId)
    {
        case "Search" : //조회 콜백
			this.dsOdr.set_enableevent(true);
			this.dsOdr.set_rowposition(this.fv_nWorkRow);
