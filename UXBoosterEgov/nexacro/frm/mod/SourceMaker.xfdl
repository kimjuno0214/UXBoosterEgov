    if( sErrMsg == "SUCC" )
    {
        //Success Script      
    } else {
        //Etc Script
    }   
    
    switch(sId)
    {
        case "SourceMaker" :		// 화면생성
			take.alert(this, "Info", "화면생성이 완료되었습니다.\nCRUD의 경우 Controller, Service Java가 생성되니 서버를 Re-Start한후 확인하세요.", "");
            break;
			
        default :
            break;
    }        
};

