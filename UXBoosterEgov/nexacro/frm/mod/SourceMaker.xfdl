this.divDetail_btnSave_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.dsBaseInfo.clearData();
	var nRow	= this.dsBaseInfo.addRow();
	var objDs	= this.dsBaseInfo;
	
