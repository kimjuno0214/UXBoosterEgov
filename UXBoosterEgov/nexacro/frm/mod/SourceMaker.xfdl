	objDs.setColumn(nRow, "FORM_LAYOUT"		, sFormLayout);
	objDs.setColumn(nRow, "CRUD_YN"			, sCrudYn);
	objDs.setColumn(nRow, "SEARCH_LAYOUT"	, sSearchLayout);
