//     if(this.dsVacationList.getColumn(e.row, "APP_TYPE") != '01') {
//         this.divDetail.form.txtApprovalMemo.set_readonly(true);
//     } else {
//         this.divDetail.form.txtApprovalMemo.set_readonly(false);
