	                  "this.divDetail.form.DivRight.form.btnTab01" : this.divDetail.form.DivRight.form.divTab01,
					  "this.divDetail.form.DivRight.form.btnTab02" : this.divDetail.form.DivRight.form.divTab02};
        var sTabId = "tab01";
